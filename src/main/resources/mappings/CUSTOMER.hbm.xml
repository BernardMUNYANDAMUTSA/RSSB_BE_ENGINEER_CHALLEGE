<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.tekana.ewallet.model">
	<class name="Customer" table="CUSTOMER_DETAILS">
		<id name="id" column="CUSTOMER_ID">
			<generator class="identity" />
		</id>
		<property name="names" column="CUSTOMER_NAMES" />
		<property name="email" column="CUSTOMER_EMAIL" />
		<property name="password" column="CUSTOMER_PASSWORD" />
	</class>
</hibernate-mapping>